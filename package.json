{
  "name": "stoicquote",
  "displayName": "Stoic Quotes",
  "description": "Show a StoicQuote modal each time you open VsCode.",
  "publisher": "mcgrealife",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:zencode.fetchStoicQuote",
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "icon": "images/stoic-quote-icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/mcgrealife/vscode-ext-stoic-quote"
  },
  "contributes": {
    "commands": [
      {
        "command": "zencode.fetchStoicQuote",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node-fetch": "^2.6.2",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "node-fetch": "^2.6.7"
  }
}